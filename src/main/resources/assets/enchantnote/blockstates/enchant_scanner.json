{
  "multipart": [
    { "when": {"facing": "north"},
      "apply": {"model": "enchantnote:block/enchant_scanner"}
    },
    { "when": {"facing": "east"},
      "apply": {"model": "enchantnote:block/enchant_scanner", "y": 90 }
    },
    { "when": {"facing": "south"},
      "apply": {"model": "enchantnote:block/enchant_scanner", "y": 180 }
    },
    { "when": {"facing": "west"},
      "apply": {"model": "enchantnote:block/enchant_scanner", "y": 270 }
    },
    { "when": {"facing": "north", "has_table_cloth": true},
      "apply": {"model": "enchantnote:block/enchant_scanner_cloth"}
    },
    { "when": {"facing": "east", "has_table_cloth": true},
      "apply": {"model": "enchantnote:block/enchant_scanner_cloth", "y": 90 }
    },
    { "when": {"facing": "south", "has_table_cloth": true},
      "apply": {"model": "enchantnote:block/enchant_scanner_cloth", "y": 180 }
    },
    { "when": {"facing": "west", "has_table_cloth": true},
      "apply": {"model": "enchantnote:block/enchant_scanner_cloth", "y": 270 }
    }
  ]
}